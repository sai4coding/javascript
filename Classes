class Calculator{
  constructor(a,b){
    this.a=a;
    this.b=b;
  }
  add(){
    console.log(this.a+this.b);
  }
  sub(){
    console.log(this.a-this.b);
  }
  multi(){
    console.log(this.a*this.b);
  }
  div(){
    console.log(this.a/this.b);
  }
}
 

let result=new Calculator(2,3);
result.add();
result.sub();
result.multi();
result.div();
